Pool.SpookyText : array
{
	foreach SpookyDialogue; _dialogue
	{
		_face = discouraged
		if "||" _in_ _dialogue
		{
			_face = SPLIT(_dialogue,"||")[0]
			_dialogue = SPLIT(_dialogue,"||")[1]
		}
		"\0%(_face)" + SpookyText(_dialogue) + "%(end('spookytext'))"
	}
}

